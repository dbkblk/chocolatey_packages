    <copyright>© Microsoft, Intel</copyright>
    <licenseUrl>https://mran.revolutionanalytics.com/assets/text/mkl-eula.txt</licenseUrl>
    <packageSourceUrl>https://github.com/dbkblk/chocolatey_packages/tree/master/microsoft-r-mkl</packageSourceUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <iconUrl>https://mran.revolutionanalytics.com/assets/img/ClarkHead.d73204d7.png</iconUrl>
    <docsUrl>https://mran.revolutionanalytics.com/documents/getting-started/</docsUrl>
    <bugTrackerUrl>https://github.com/RevolutionAnalytics/RRO/issues</bugTrackerUrl>
    <projectSourceUrl>https://github.com/RevolutionAnalytics/RRO</projectSourceUrl>
    <releaseNotes>https://github.com/RevolutionAnalytics/RRO/releases/tag/MRO-3.3.0</releaseNotes>
    <dependencies>
      <dependency id="autohotkey.portable" version="1.1"/>
